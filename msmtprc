account disqus
host smtp.gmail.com
port 587
protocol smtp
auth plain
from cole@disqus.com
user cole@disqus.com
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account northernbloc
host mail.northernbloc.org
port 587
protocol smtp
auth plain
from cole@northernbloc.org
user cole@northernbloc.org
tls on
tls_starttls off
tls_fingerprint 72:BE:7E:78:F3:E6:E0:64:30:37:D0:C8:67:47:CF:4B:5C:D7:64:57

account default: northernbloc
